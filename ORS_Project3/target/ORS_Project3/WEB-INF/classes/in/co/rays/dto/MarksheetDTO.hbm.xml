<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name = "com.sunilOS.ORSProject3.dto.MarksheetDTO" table = "ST_MARKSHEET">	
		
		<id name = "id" type = "java.lang.Long">
		<column name = "ID"/>
		<generator class = "increment"/>
		</id>
		
		<property name="rollNo" type="java.lang.String">
			<column name="ROLL_NO" />
		</property>
		<property name="studentId" type="java.lang.Long">
			<column name="STUDENT_ID" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" />
		</property>

		<property name="physics" type="java.lang.Integer">
			<column name="PHYSICS" />
		</property>

		<property name="chemistry" type="java.lang.Integer">
			<column name="CHEMISTRY" />
		</property>

		<property name="maths" type="java.lang.Integer">
			<column name="MATHS" />
		</property>
		<property name="createdBy" type="java.lang.String">
			<column name="CREATED_BY" />
		</property>
		<property name="modifiedBy" type="java.lang.String">
			<column name="MODIFIED_BY" />
		</property>
		<property name="createdDatetime" type="java.sql.Timestamp">
			<column name="CREATED_DATETIME" />
		</property>
		<property name="modifiedDatetime" type="java.sql.Timestamp">
			<column name="MODIFIED_DATETIME" />
		</property>
	</class>
</hibernate-mapping>
