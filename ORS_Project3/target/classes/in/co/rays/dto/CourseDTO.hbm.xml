<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name = "com.sunilOS.ORSProject3.dto.CourseDTO" table = "ST_COURSE">	
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="increment" />
		</id>
		<property name="courseName" type="java.lang.String">
			<column name="COURSE_NAME" />
		</property>
		<property name="courseDuration" type="java.lang.String">
			<column name="COURSE_DURATION" />
		</property>
		<property name="courseDescription" type="java.lang.String">
			<column name="COURSE_DESCRIPTION" />
		</property>
		<property name="createdBy" type="java.lang.String">
			<column name="CREATED_BY" />
		</property>
		<property name="modifiedBy" type="java.lang.String">
			<column name="MODIFIED_BY" />
		</property>
		<property name="createdDatetime" type="java.sql.Timestamp">
			<column name="CREATED_DATETIME" />
		</property>
		<property name="modifiedDatetime" type="java.sql.Timestamp">
			<column name="MODIFIED_DATETIME" />
		</property>
	</class>
</hibernate-mapping>
